{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
  "outDir": "dist",
  "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
